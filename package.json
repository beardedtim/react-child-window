{
  "name": "react-child-window",
  "version": "0.0.1",
  "main": "index.js",
  "author": "beardedtim <timiroberts@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:w": "jest --watch",
    "lint": "echo \" You have not set up a lint thing!\"; exit 0",
    "test:ci": "jest --coverage && codecov",
    "ci": "yarn test:ci && yarn lint",
    "build": "yarn ci && webpack -p",
    "pre-commit-message": "echo \" Ensuring CI will pass before we event think about commiting this...\"; exit 0"
  },
  "pre-commit": [
    "pre-commit-message",
    "test",
    "lint"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              ">1%",
              "last 2 version"
            ]
          },
          "useBuiltIns": true
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "codecov": "^2.2.0",
    "enzyme": "^2.9.1",
    "jest": "^20.0.4",
    "pre-commit": "^1.2.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.0.0"
  }
}
